<extension xmlns="http://ns.adobe.com/air/extension/3.4">
	<id>com.adobe.ane.gameCenter</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libgameCenter.a</nativeLibrary>
				<initializer>gameCenterExtInitializer</initializer>
				<finalizer>gameCenterExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
        <platform name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libgameCenter.a</nativeLibrary>
				<initializer>gameCenterExtInitializer</initializer>
				<finalizer>gameCenterExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="default"> 
			<applicationDeployment/> 
		</platform>
	</platforms>
</extension>
