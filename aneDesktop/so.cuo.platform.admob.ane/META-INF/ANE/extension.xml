<extension xmlns="http://ns.adobe.com/air/extension/3.9">
    <id>so.cuo.platform.admob</id>
    <versionNumber>1</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>admob4.0.jar</nativeLibrary>
                <initializer>so.cuo.platform.admob.AdmobExtension</initializer>
                <finalizer>so.cuo.platform.admob.AdmobExtension</finalizer>
            </applicationDeployment>
        </platform>
		<platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>admob4.0.a</nativeLibrary>
                <initializer>admobExtInitializer</initializer>
                <finalizer>admobExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
